{
  "npmScope": "nx-nextjs",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "app": {
      "tags": []
    },
    "app-e2e": {
      "tags": [],
      "implicitDependencies": ["app"]
    },
    "nextjs": {
      "tags": []
    },
    "nextjs-e2e": {
      "tags": [],
      "implicitDependencies": ["nextjs"]
    },
    "shared-components": {
      "tags": []
    },
    "shared-components-e2e": {
      "tags": [],
      "implicitDependencies": ["shared-components"]
    }
  }
}
