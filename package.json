    "tailwindcss": "^3.0.3",
    "ts-jest": "27.0.5",
    "ts-node": "9.1.1",
    "tslib": "^2.1.0",
    "typescript": "4.4.2",
    "url-loader": "^3.0.0",
    "webpack": "^5.59.1"
  }
}
